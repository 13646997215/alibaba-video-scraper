{
  "version": 2,
  "builds": [
    {
      "src": "api/scrape.py",
      "use": "@vercel/python"
    },
    {
      "src": "web",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/scrape.py"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}
